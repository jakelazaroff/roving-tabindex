{
  "formatter": {
    "indentStyle": "space",
    "indentWidth": 2,
    "lineWidth": 100
  },
  "javascript": {
    "formatter": {
      "arrowParentheses": "asNeeded",
      "bracketSpacing": true,
      "quoteProperties": "asNeeded"
    }
  },
  "linter": {
    "rules": {
      "complexity": {
        "noThisInStatic": "off"
      },
      "style": {
        "useSingleVarDeclarator": "off"
      }
    }
  }
}
